{
    "name": "cheat_sheet",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite --host",
        "set_dev_config": "[ -f \"./src/config.json\" ] && echo \"Config already exists\" || cp ./src/config-example.json ./src/config.json && sed -i \"\" \"s/\\[YOUR CURRENT BRANCH\\]/$(git branch --show-current)/g\" ./src/config.json",
        "build": "vite build",
        "preview": "vite preview",
        "test:unit": "vitest --environment jsdom --root src/",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
    },
    "dependencies": {
        "axios": "^1.2.0",
        "clipboard": "^2.0.11",
        "fuse.js": "^6.6.2",
        "highlight.js": "^11.7.0",
        "markdown-it": "^13.0.1",
        "natural": "^5.2.3",
        "node-lemmatizer": "^1.0.3",
        "pinia": "^2.1.3",
        "vue": "^3.2.41"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.1.4",
        "@vitejs/plugin-vue": "^3.1.2",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/test-utils": "^2.1.0",
        "eslint": "^8.22.0",
        "eslint-plugin-vue": "^9.3.0",
        "jsdom": "^20.0.1",
        "prettier": "^2.7.1",
        "pug": "^3.0.2",
        "pug-plain-loader": "^1.1.0",
        "sass": "^1.56.1",
        "sass-loader": "^10.4.1",
        "vite": "^3.1.8",
        "vitest": "^0.24.3"
    }
}
